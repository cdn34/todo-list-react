{
  "name": "list-app-react",
  "version": "0.1.0",
  "description": "Simple todo list app.",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node client/tools/nodeVersionCheck.js",
    "setup": "node client/tools/setup/setupMessage.js && npm install && node client/tools/setup/setup.js",
    "start-message": "babel-node client/tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "start": "npm-run-all --parallel test:watch lint:watch open:nodedev open:src",
    "open:nodedev": "node server/server.js",
    "open:src": "babel-node client/tools/srcServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node client/tools/build.js",
    "test": "mocha client/tools/testSetup.js client/src/**/*.spec.js --reporter progress",
    "test:watch": "npm run test -- --watch",
    "analyze-bundle": "babel-node ./client/tools/analyzeBundle.js"
  },
  "author": "Carlos Nogueira",
  "license": "MIT",
  "dependencies": {
    "axios": "0.15.3",
    "body-parser": "^3.0.0",
    "co": "4.6.0",
    "compression": "1.6.2",
    "koa": "2.0.0",
    "koa-bodyparser": "3.0.0",
    "koa-compress": "2.0.0",
    "koa-convert": "1.2.0",
    "koa-mount": "2.0.0",
    "koa-router": "7.0.1",
    "koa-static": "3.0.0",
    "koa-views": "5.0.1",
    "mongoose": "4.8.1",
    "object-assign": "4.1.0",
    "path": "0.12.7",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-redux": "5.0.1",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "6.5.4",
    "babel-cli": "6.18.0",
    "babel-core": "6.20.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.11",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.16.0",
    "babel-register": "6.18.0",
    "browser-sync": "2.18.5",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.15",
    "cross-env": "3.1.3",
    "css-loader": "0.26.1",
    "enzyme": "2.6.0",
    "eslint": "3.12.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.8.0",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.1",
    "http-proxy-middleware": "0.17.3",
    "isparta": "4.0.0",
    "istanbul": "0.4.4",
    "jsdom": "9.8.3",
    "json-loader": "0.5.4",
    "mocha": "3.2.0",
    "mockdate": "2.0.1",
    "node-sass": "4.0.0",
    "npm-run-all": "3.1.2",
    "open": "0.0.5",
    "postcss-loader": "1.2.1",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.4.1",
    "redux-immutable-state-invariant": "1.2.4",
    "replace": "0.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.1.0",
    "sinon": "1.17.6",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-bundle-analyzer": "2.1.1",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.13.2",
    "webpack-md5-hash": "0.0.5"
  },
  "keywords": [],
